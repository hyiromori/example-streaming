{
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true
  },
  "extends": "Standard",
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2019
  },
  "rules": {
    "arrow-parens": [
      0,
      "always"
    ]
  }
}
